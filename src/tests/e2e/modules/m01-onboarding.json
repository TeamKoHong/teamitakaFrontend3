{
  "module_id": "M01",
  "module_name": "온보딩 & 회원가입",
  "features": [
    {
      "feature_id": "M01-F01",
      "feature_name": "온보딩 화면",
      "route": "/",
      "test_scenarios": [
        {
          "id": "M01-F01-01",
          "description": "신규 유저에게 온보딩 화면이 표시되는가?",
          "user_state": "US01",
          "priority": "critical",
          "steps": [
            { "action": "navigate", "target": "/" },
            { "action": "wait", "selector": ".onboarding-page-container", "timeout": 5000 },
            { "action": "screenshot", "name": "onboarding-initial" }
          ],
          "validations": [
            { "type": "element_visible", "selector": ".onboarding-page-container" },
            { "type": "url_equals", "expected": "/" }
          ],
          "expected_result": "처음 앱 설치 시 온보딩 슬라이드 표시"
        },
        {
          "id": "M01-F01-02",
          "description": "온보딩 로고 이미지가 표시되는가?",
          "user_state": "US01",
          "priority": "high",
          "steps": [
            { "action": "navigate", "target": "/" },
            { "action": "wait", "selector": ".onboarding-page-container", "timeout": 3000 }
          ],
          "validations": [
            { "type": "element_visible", "selector": "img[alt='onboarding-page-logo'], .onboarding-page-container img" }
          ],
          "expected_result": "온보딩 로고 이미지 표시"
        },
        {
          "id": "M01-F01-03",
          "description": "온보딩 화면에서 자동으로 로그인 페이지로 이동하는가?",
          "user_state": "US01",
          "priority": "high",
          "steps": [
            { "action": "navigate", "target": "/" },
            { "action": "wait_for_navigation", "timeout": 5000 }
          ],
          "validations": [
            { "type": "url_contains", "expected": "/login" }
          ],
          "expected_result": "3초 후 자동으로 로그인 페이지로 이동"
        }
      ]
    },
    {
      "feature_id": "M01-F02",
      "feature_name": "로그인",
      "route": "/login",
      "test_scenarios": [
        {
          "id": "M01-F02-01",
          "description": "학교 이메일 입력 필드가 존재하는가?",
          "user_state": "US01",
          "priority": "critical",
          "steps": [
            { "action": "navigate", "target": "/login" },
            { "action": "wait", "selector": ".login-page-container", "timeout": 3000 },
            { "action": "screenshot", "name": "login-page" }
          ],
          "validations": [
            { "type": "element_visible", "selector": ".input-field[type='email'], .input-field:first-of-type" },
            { "type": "element_enabled", "selector": ".input-field" }
