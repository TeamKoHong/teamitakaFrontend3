  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^react-router-dom$": "<rootDir>/src/test-utils/react-router-dom-mock.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "sass": "^1.90.0",
    "sass-loader": "^13.3.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "@reach/portal": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "@reach/utils": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "@xstate/react": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
